{
  "name": "inahurry",
  "version": "1.0.0",
  "description": "定位目前的geolocation，分析出距離最近的火車站，在由node.js去爬台鐵/高鐵的火時刻表，搜尋出最近的發車資訊。",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf server/assets",
    "webpackDev": "node webpack-server.js ",
    "serverDev": "nodemon server --exec babel-node --watch server/",
    "webpackDeploy": "npm run clean && webpack -p --progress --config webpack.production.config.js",
    "server": "NODE_ENV=production babel-node server",
    "dev": "parallelshell 'npm run serverDev' 'npm run webpackDev'"
  },
  "repository": "https://github.com/duncan60/inahurry/",
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "webpack",
    "node.js",
    "server",
    "server-side"
  ],
  "author": "Duncan Du <tmduncan60@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.3",
    "isomorphic-fetch": "^2.2.0",
    "moment": "^2.10.3",
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^1.0.0",
    "request": "^2.61.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.14.0",
    "babel-preset-react": "^6.11.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cheerio": "^0.19.0",
    "css-loader": "~0.23.1",
    "csswring": "^3.0.5",
    "envify": "~1.2.1",
    "eslint": "2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.0",
    "express": "^4.13.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.4",
    "jsx-loader": "~0.10.2",
    "node-sass": "^3.2.0",
    "nodemon": "^1.4.1",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.1",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "request": "^2.61.0",
    "rimraf": "^2.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.1",
